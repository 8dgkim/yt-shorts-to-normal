{
    "manifest_version": 3,
    "name": "YouTube Shorts to Normal Video Extension",
    "description": "Automatically Switches YouTube Shorts to Normal Video Interface",
    "version": "1.1",
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["main.js"]
        }
    ],
    "permissions": [
        "notifications"
    ]
}